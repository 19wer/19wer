import java.util.Scanner;
public class xc {

	public static void main(String[] args) {
	Scanner input=new Scanner(System.in);
	Integer[] n=new Integer[120];
	int t=0,temp;
	for(int i=1;i<=100;i++)
		n[i]=0;
	System.out.print("Enter the integer betweens 1 and 100: ");
	while(true) {
	   temp=input.nextInt();
	   if(temp==0)
		   break;
	   n[temp]++;
	   t++;
	}
	for(int i=1;i<=100;i++) {
		if(n[i]!=0)
			System.out.println(i+" occurs "+n[i]+" times");		
	}

	}

}

