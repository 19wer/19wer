import java.util.Scanner;
public class hello {
	public static int linearSearch(int[] list,int key) {
        for(int i=0;i<list.length;i++) {
            if(key==list[i])
            	return 1;		
        }
        return -1;
	}
	public static void main(String[] args) {
		int[] list= {1,2,3,4,5};
int i=linearSearch(list,4);
System.out.println(i);
	}
}
